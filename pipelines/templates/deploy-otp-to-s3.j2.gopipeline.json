    "parameters": [],
    "materials": [
        {% for name in upstream_pipelines %}
        {
            "pipeline": "{{ name }}",
            "stage": "build",
            "name": "{{ name }}",
            "type": "dependency"
        },
        {% endfor %}
        {
            "url": "https://github.com/nextl00p/grisp-gocd.git",
            "branch": "master",
            "shallow_clone": false,
            "filter": {
                "ignore": [],
                "whitelist": []
            },
            "destination": ".gocd",
            "auto_update": true,
            "name": "grisp-gocd",
            "type": "git"
        }
    ],
    "stages": [
        {
            "name": "deploy",
            "fetch_materials": true,
            "never_cleanup_artifacts": false,
            "clean_working_directory": true,
            "approval": {
                "type": "success",
                "users": [],
                "roles": []
            },
            "environment_variables": [],
            "jobs": [
                {
                    "name": "deploy",
                    "environment_variables": [],
                    "tabs": [],
                    "resources": [],
                    "artifacts": [
                        {
                            "source": "deploy_log",
                            "destination": "",
                            "type": "build"
                        }
                    ],
                    "properties": [],
                    "run_instance_count": "0",
                    "timeout": 0.0,
                    "tasks": [
                        {% for name in upstream_pipelines %}
                        {
                            "source": "otp",
                            "is_source_a_file": false,
                            "destination": "",
                            "pipeline": "{{ name }}",
                            "stage": "build",
                            "job": "build_grisp_project",
                            "artifact_origin": "gocd",
                            "run_if": "passed",
                            "type": "fetch"
                        },
                        {% endfor %}
                        {
                            "command": "/bin/bash",
                            "timeout": -1.0,
                            "arguments": [
                                ".gocd/deploy-otp.sh"
                            ],
                            "run_if": "passed",
                            "type": "exec"
                        }
                    ]
                }
            ]
        }
    ]
}
